{
  "version": "0.2",
  "language": "en",
  "ignorePaths": [
    "**/node_modules/**",
    "**/.git/**",
    "**/.gitignore/**",
    "**/*.config.js",
    "**/*.config.ts",
    "**/.eslintrc.*",
    "**/.prettierrc",
    "**/.stylelintrc",
    "**/package.json",
    "**/package-lock.json",
